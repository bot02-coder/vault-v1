generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Manga {
  id          String   @id @default(cuid())
  name        String
  description String
  tags        String[]
  coverUrl    String
  destUrl     String
  createdAt   DateTime @default(now())
}
